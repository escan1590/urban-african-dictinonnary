  "scripts": {
    "start": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test-ts": "jasmine-ts",
    "test": "db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "build": "tsc --build",
    "test2": "tsc --build && npm run jasmine",
    "jasmine": "jasmine",
    "jasmine-ts": "jasmine-ts",
    "db-migrate": "db-migrate",
    "db-mig-up-test": "db-migrate --env test up",
    "db-mig-down-test": "db-migrate --env test down",
    "prettylint": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.7.6",
    "@types/node": "^15.6.1",
    "@types/pg": "^8.6.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0"
  }
